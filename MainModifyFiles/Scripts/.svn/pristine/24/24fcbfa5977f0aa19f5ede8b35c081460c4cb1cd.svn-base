#if UNITY_EDITOR
using UnityEngine;
using System.Collections;
using System.Collections.Generic;

public class ExperiencePathManager
{
	public static List< string > loadResPath = new List<string>();
	
	public static void AddResPath( string path )
	{
		if( !loadResPath.Contains( path ))
		{
			loadResPath.Add( path );
		}
	}
}

#endif